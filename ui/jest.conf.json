{
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.ts",
      "\\.(css|less)$": "<rootDir>/test/style-mock.ts"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "test/.+\\.(test|spec)\\.(tsx?|jsx?)$",
    "setupTestFrameworkScriptFile": "<rootDir>/node_modules/core-js/client/shim.js"
  }